{
  "name": "show-me-your-enzo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ignore-path .gitignore",
    "seed:submissions": "node db/seed.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.11.9",
    "classnames": "^2.2.6",
    "date-fns": "^2.21.1",
    "eslint": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-promise": "*",
    "framer-motion": "^4.1.11",
    "mapbox-gl": "^2.2.0",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-map-gl": "^6.1.12",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@samtgarson/eslint-config": "^2.4.0",
    "@types/react": "^17.0.4",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "calipers": "^2.0.1",
    "calipers-png": "^2.0.0",
    "dotenv": "^9.0.1",
    "form-data": "^4.0.0",
    "postcss": "^8.2.13",
    "random-location": "^1.1.3",
    "sass": "^1.32.11",
    "tailwindcss": "^2.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "env": {
      "es2020": true
    },
    "ignorePatterns": [
      "*.config.js"
    ],
    "extends": [
      "@samtgarson/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "import/extensions": [
        "error",
        {
          "scss": "always"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "extends": [
          "@samtgarson/eslint-config/typescript",
          "@samtgarson/eslint-config/react"
        ]
      },
      {
        "files": [
          "db/seed.js"
        ],
        "rules": {
          "import/no-extraneous-dependencies": 0
        }
      }
    ]
  }
}
